	}
	
	
	.responsive .grid-column {
		float: none;
		width: auto;
		margin: 0;
		border: 0;
		border-top: 1px solid #d9d9d9;
	}
	.responsive .grid-column:first-child {
		border-top: 0;
	}
	.responsive .grid-column:first-child ul li:last-child,
	.responsive .grid-column:first-child ul li:last-child > a,
