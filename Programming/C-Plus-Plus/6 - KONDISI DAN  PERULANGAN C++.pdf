	}
	.responsive ul ul li:first-child > a {
		border-radius: 0;
	}
	.responsive ul ul .dropdown:first-child:hover {
		border-radius: 0;	
	}
	.responsive ul ul .dropdown:first-child:hover > a {
		border-radius: 0;
	}
	
	.responsive ul ul ul a {
		padding-left: 76px;
	}
	

	.responsive .right {
